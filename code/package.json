{
  "name": "hs_app-integracao-senior",
  "version": "1.0.0",
  "description": "",
  "main": "src/main/server.ts",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "clear": "rm -rf dist",
    "clear:all": "rm -rf dist node_modules && npm run clear",
    "clear:logs": "rm -rf storage/logs/debug/* storage/logs/error/* storage/logs/info/*",
    "lint": "eslint \"**/*.ts\" --ignore-pattern node_modules/ ",
    "prestart": "npm run clear && npm run build",
    "start": "node ./dist/main/server.js",
    "start:dev": "tsx watch --no-cache src/main/server.ts",
    "test": "vitest",
    "test:coverage": "rm -rf ./coverage && vitest run --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-decorators": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@faker-js/faker": "^8.3.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-istanbul": "^1.1.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "prettier": "^3.1.1",
    "shelljs": "^0.8.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jsdoc-swagger": "^1.8.0",
    "moment-timezone": "^0.5.43",
    "node-ts-cache": "^4.4.0",
    "node-ts-cache-storage-memory": "^4.4.0",
    "swagger-ui-express": "^5.0.0"
  }
}
