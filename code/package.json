{
  "name": "hs_app-integracao-senior",
  "version": "1.0.0",
  "description": "",
  "main": "src/main/server.ts",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "clear": "rm -rf dist",
    "clear:all": "rm -rf dist node_modules && npm run clear",
    "clear:logs": "rm -rf storage/logs/debug/* storage/logs/error/* storage/logs/info/*",
    "lint": "eslint \"**/*.ts\" --ignore-pattern node_modules/ ",
    "prestart": "npm run clear && npm run build",
    "start": "node ./dist/main/server.js",
    "start:dev": "tsx watch --no-cache src/main/server.ts",
    "test": "vitest",
    "test:coverage": "rm -rf ./coverage && vitest run --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@faker-js/faker": "^7.6.0",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/moment-timezone": "^0.5.30",
    "@types/node-cron": "^3.0.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitest/coverage-istanbul": "^0.29.8",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.3",
    "shelljs": "^0.8.5",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jsdoc-swagger": "^1.8.0",
    "moment-timezone": "^0.5.43",
    "node-ts-cache": "^4.4.0",
    "node-ts-cache-storage-memory": "^4.4.0",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0"
  }
}
